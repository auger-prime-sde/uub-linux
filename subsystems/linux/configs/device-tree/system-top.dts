/dts-v1/;
/include/ "system-conf.dtsi"
/ {

/*
 *  Stupid PetaLinux installer doesn't propagate console choice.
 */
 
	chosen {
		bootargs = "console=ttyPS1,9600 earlyprintk";
	};
};
/*
 * Stupid PetaLinux installer uses a flash that's not actually
 * listed in the MTD subsystem list!
 */
&qspi {
	flash0: flash@0 {
		compatible = "micron,n25q00";
		spi-max-frequency = <25000000>;
        };
};	

&spi0 {
        is-decoded-cs = <1>;
	num-cs = <5>;
	status = "okay";
	spidev@0 {
	  compatible = "spidev";
	  reg = <0>;
	  spi-max-frequency = <50000000>;
	};
	spidev@1 {
	  compatible = "spidev";
	  reg = <1>;
	  spi-max-frequency = <50000000>;
	};
	spidev@2 {
	  compatible = "spidev";
	  reg = <2>;
	  spi-max-frequency = <50000000>;
	};
	spidev@3 {
	  compatible = "spidev";
	  reg = <3>;
	  spi-max-frequency = <50000000>;
	};
	spidev@4 {
	  compatible = "spidev";
	  reg = <4>;
	  spi-max-frequency = <50000000>;
	};
};

&spi1 {
        spidev@0 {
	  compatible = "spidev";
	  reg = <0>;
	  spi-max-frequency = <50000000>;
	};
};

&gem0 {
        phy-handle = <&phy0>;
	ps7_ethernet_0_mdio: mdio {
	  status = "disabled";
	};
	phy0: phy@0 {
	  compatible = "marvell,88e1510";
	  device_type= "ethernet-phy";
 	  reg = <0>;	  
	};
};